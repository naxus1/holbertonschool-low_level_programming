/**
 * main - Entry point
 * @argc:count arguments of input
 * @argv: pointer of arguments of input
 * Return: Always 0 (Success)
 */

void *_calloc(unsigned int nmemb, unsigned int size)
{
	unsigned int *myarray;
	int i;
 
	if(nmemb <= 0 || size <= 0)
		return (NULL);

	myarray = malloc(sizeof(int) * nmemb);

	if(myarray == NULL)
		return (NULL);

	for(i = 0; i < size; i++)
	{
		myarray[i] = 0;
	}

	return (myarray);
}
