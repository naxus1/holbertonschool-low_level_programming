#include <stdio.h>
#include "dog.h"

/**
 * main - check the code for Holberton School students.
 *
 * Return: Always 0.
 */

/ * *
* struct dog - Dog data
* @a: name dog
* @b: age dog
* @c: owner dog
*
* Descripci√≥n: Dog data
* /struct dog
   {
	   char *name;
	   double age;
	   char *owner;
};

int main(void)
{
	struct dog my_dog;

	my_dog.name = "Django";
	my_dog.age = 3.5;
	my_dog.owner = "Jay";
	printf("My name is %s, and I am %.1f :) - Woof!\n", my_dog.name, my_dog.age);
	return (0);
}
